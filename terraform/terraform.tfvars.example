# Example terraform.tfvars file
# Copy this file to terraform.tfvars and modify the values as needed

resource_group_name = "ahora-caigo-rg"
location            = "East US"
storage_account_name = "ahoracaigostorage"
app_service_plan_name = "ahora-caigo-app-plan"
