static_web_app_name  = "ahora-caigo-frontend"
app_service_sku      = "F1"  # Free tier - cheapest option

tags = {
  Environment = "Production"
  Project     = "Ahora Caigo"
  ManagedBy   = "Terraform"
  Owner       = "DevOps Team"
} 